# Environment Variables for Football App
# Copy this file to .env and replace with your actual values

# ===========================================
# REQUIRED: Gemini AI API Key
# ===========================================
# Get your API key from: https://aistudio.google.com/
# This is REQUIRED for AI features to work
# NEVER commit your real API key to git!
EXPO_PUBLIC_GEMINI_API_KEY=your_gemini_api_key_here

# ===========================================
# OPTIONAL: Backend Configuration
# ===========================================
# Backend URL (current MockAPI is working)
EXPO_PUBLIC_BACKEND_URL=https://6772048bee76b92dd490f044.mockapi.io/api/v1

# ===========================================
# APP CONFIGURATION
# ===========================================
# App environment (development, staging, production)
EXPO_PUBLIC_APP_ENV=development

# Debug mode (true/false)
EXPO_PUBLIC_DEBUG_MODE=true

# ===========================================
# SETUP INSTRUCTIONS:
# ===========================================
# 1. Copy this file: cp .env.example .env
# 2. Get Gemini API key from https://aistudio.google.com/
# 3. Replace 'your_gemini_api_key_here' with your actual API key
# 4. Restart Expo development server
# ===========================================
